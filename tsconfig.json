{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "ESNext",
    "module": "CommonJS",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "strictPropertyInitialization": false,
    "strict": true,
    "types": [
      "node",
      "jest"
    ],
    "paths": {
      "@*": [
        "src/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx"
  }
}